{
    "version": 2,
    "builds": [
      {
        "src": "front_page.html",
        "use": "@vercel/static"
      },
      {
        "src": "projects.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/front_page.html"
      },
      {
        "src": "/projects",
        "dest": "/projects.html"
      }
    ]
  }